{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFPratham"
		},
		"AzureDataLakeStorageCSV_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageCSV'"
		},
		"AzureDataLakeStorageCSV_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sadatapratham240525.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage388_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/e84d8697-ef3e-4296-9629-cdeb0c1df544/resourceGroups/rg-dataplatform-assessment/providers/Microsoft.Storage/storageAccounts/sadatapratham240525"
		},
		"AzureDataLakeStorage388_properties_groupId": {
			"type": "string",
			"defaultValue": "dfs"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFPratham_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageCSV_accountKey": {
						"value": "[parameters('AzureDataLakeStorageCSV_accountKey')]"
					},
					"AzureDataLakeStorage388_properties_privateLinkResourceId": {
						"value": "[parameters('AzureDataLakeStorage388_properties_privateLinkResourceId')]"
					},
					"AzureDataLakeStorage388_properties_groupId": {
						"value": "[parameters('AzureDataLakeStorage388_properties_groupId')]"
					},
					"AzureDataLakeStorageCSV_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageCSV_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}